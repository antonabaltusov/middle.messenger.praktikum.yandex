{
  "name": "my-webpack-project",
  "version": "1.0.0",
  "description": "My webpack project",
  "scripts": {
    "dev": "parcel ./src/index.html --no-cache",
    "prod": "parcel build ./src/index.html",
    "start": "parcel build ./src/index.html && node ./server.js",
    "test": ".github/workflows/tests.yml",
    "test-jest": "jest",
    "lint": "node_modules/.bin/eslint . --fix",
    "build": "webpack --mode=production --node-env=production",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "watch": "webpack --watch",
    "serve": "webpack serve",
    "mocha": "ts-mocha --paths 'src/**/*.spec.ts' --require jsdom-global --require mock-local-storage  --require mocha-dom --require tests/mock.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm-run-all test lint:all",
      "pre-push": "npm run other command"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antonabaltusov/middle.messenger.praktikum.yandex.git"
  },
  "alias": {
    "utils": "./src/utils",
    "components": "./src/components",
    "services": "./src/services",
    "helpers": "./src/helpers",
    "pages": "./src/pages",
    "api": "./src/api",
    "blocks": "./src/blocks",
    "assets": "./src/assets",
    "styles": "./src/styles"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/antonabaltusov/middle.messenger.praktikum.yandex/issues"
  },
  "homepage": "https://github.com/antonabaltusov/middle.messenger.praktikum.yandex#readme",
  "devDependencies": {
    "@babel/core": "7.20.5",
    "@babel/preset-typescript": "7.18.6",
    "@babel/register": "7.18.9",
    "@parcel/transformer-inline-string": "2.7.0",
    "@parcel/transformer-sass": "2.7.0",
    "@types/chai": "4.3.4",
    "@types/expect": "24.3.0",
    "@types/jest": "29.2.4",
    "@types/mocha": "10.0.1",
    "@typescript-eslint/eslint-plugin": "5.38.0",
    "@typescript-eslint/parser": "5.38.0",
    "@webpack-cli/generators": "3.0.1",
    "ajv": "8.11.2",
    "autoprefixer": "10.4.9",
    "babel-loader": "9.1.0",
    "babel-plugin-dynamic-import-node": "2.3.3",
    "babel-plugin-webpack-loaders": "0.9.0",
    "chai": "4.3.7",
    "css-loader": "6.7.2",
    "eslint": "8.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "html-webpack-plugin": "5.5.0",
    "husky": "8.0.2",
    "identity-obj-proxy": "3.0.0",
    "jsdom": "20.0.3",
    "jsdom-context-require": "4.0.6",
    "jsdom-global": "3.0.2",
    "mini-css-extract-plugin": "2.7.2",
    "mocha": "10.2.0",
    "mocha-dom": "1.0.0",
    "mock-local-storage": "1.1.23",
    "parcel": "2.7.0",
    "postcss": "8.4.16",
    "postcss-import": "15.0.0",
    "postcss-loader": "7.0.2",
    "postcss-nested": "5.0.6",
    "prettier": "2.8.1",
    "style-loader": "3.3.1",
    "stylelint": "14.14.1",
    "ts-jest": "29.0.3",
    "ts-loader": "9.4.2",
    "ts-mocha": "10.0.0",
    "ts-node": "9.1.1",
    "tsconfig-paths": "4.1.1",
    "typescript": "4.8.3",
    "webpack": "5.75.0",
    "webpack-cli": "5.0.1",
    "webpack-dev-server": "4.11.1"
  },
  "dependencies": {
    "chai-spies": "1.0.0",
    "express": "4.18.1",
    "handlebars": "4.7.7",
    "module-alias": "2.2.2",
    "nanoid": "4.0.0",
    "sass": "1.56.2",
    "sass-loader": "13.2.0",
    "uuid": "9.0.0"
  }
